<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<AnchorPane id="AnchorPane" prefHeight="770.0" prefWidth="870.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sushigo.PreGameController">
    <children>
        
        <ImageView fitHeight="770.0" fitWidth="870.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="file:src/images/background.png" />
            </image>
        </ImageView>
        
        
        <Button id="plybutt" layoutX="154.0" layoutY="498.0" onAction="#goAddPlay" prefHeight="84.0" prefWidth="180.0">
            <graphic>
                <ImageView fx:id="startButton" fitHeight="74.0" fitWidth="195.0">
                    <image>
                        <Image url="file:src/images/start.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        
        <Button id="insbutt" layoutX="521.0" layoutY="498.0" onAction="#goInstructions">
            <graphic>
                <ImageView fx:id="insturctionsButton" fitHeight="74.0" fitWidth="195.0">
                    <image>
                        <Image url="file:src/images/instructions.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
